<div class="section">
	<%= content_tag( 'h1', "#{@relationship.display_label} Edit".html_safe ) %>
</div>

<%= form_for @relationship, :url => { :action => 'update', :relationship_to_update => @relationship } do |f| %>
	<%= render :partial => 'form', :object => f %>
  	<%= submit_tag( 'Update', :class => 'submit' ) %>
<% end %>
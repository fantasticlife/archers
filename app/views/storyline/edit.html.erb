<div class="section">
	<%= content_tag( 'h1', "#{link_to( @storyline.title, storyline_show_url( :storyline => @storyline.guid ) )} Edit".html_safe ) %>
</div>

<%= form_for @storyline, :url => { :action => 'update', :storyline_to_update => @storyline.guid  } do |f| %>
	<%= render :partial => 'form', :object => f %>
  	<%= submit_tag( 'Update', :class => 'submit' ) %>
<% end %>
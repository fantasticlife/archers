<div class="section">
	<%= content_tag( 'h1', "#{link_to( @universe.title, universe_list_url )} Edit".html_safe ) %>
</div>

<%= form_for @universe, :url => { :action => 'update', :universe_to_update => @universe.guid } do |f| %>
	<%= render :partial => 'form', :object => f %>
  	<%= submit_tag( 'Update', :class => 'submit' ) %>
<% end %>
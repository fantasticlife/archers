<div class="section">
	<%= content_tag( 'h1', "#{link_to( @crew.name, crew_show_url( :crew => @crew ) )} Edit".html_safe ) %>
</div>

<%= form_for @crew, :url => { :action => 'update', :crew_to_update => @crew } do |f| %>
	<%= render :partial => 'form', :object => f %>
  	<%= submit_tag( 'Update', :class => 'submit' ) %>
<% end %>